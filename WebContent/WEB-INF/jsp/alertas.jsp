<section class="panel">
    <div class="panel-body">
        <div class="alert alert-block alert-danger fade in" style="display: none;">
            <button data-dismiss="alert" class="close close-sm" type="button">
                <i class="fa fa-times"></i>
            </button>
            <strong>Error! </strong> ${error}
        </div>
        <div class="alert alert-success fade in" style="display: none;">
            <button data-dismiss="alert" class="close close-sm" type="button">
                <i class="fa fa-times"></i>
            </button>
            <strong>Correcto! </strong> ${correcto}
        </div>
        <div class="alert alert-info fade in" style="display: none;">
            <button data-dismiss="alert" class="close close-sm" type="button">
                <i class="fa fa-times"></i>
            </button>
            <strong>Informacion! </strong> ${info}
        </div>
        <div class="alert alert-warning fade in" style="display: none;">
            <button data-dismiss="alert" class="close close-sm" type="button">
                <i class="fa fa-times"></i>
            </button>
            <strong>Peligro! </strong> ${peligro}
        </div>

    </div>
</section>